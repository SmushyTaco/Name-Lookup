{
  "schemaVersion": 1,
  "id": "name_lookup",
  "version": "${version}",
  "name": "Name Lookup",
  "description": "This mod allows you to look up a player's username history with the use of /namelookup.",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/name-lookup/issues/",
    "sources": "https://github.com/SmushyTaco/Name-Lookup/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/name_lookup/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.name_lookup.NameLookupClient"
      }
    ],
    "server": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.name_lookup.NameLookupServer"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  }
}